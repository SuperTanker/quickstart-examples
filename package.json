{
  "name": "@tanker/ui-demos",
  "version": "1.6.0",
  "main": "javascript/server/index.js",
  "description": "Example applications for Tanker.io",
  "repository": "git@github.com:SuperTanker/tanker-ui-demos.git",
  "author": "Tanker Team",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "javascript/apps/code-demo",
    "javascript/apps/node-demo",
    "javascript/apps/ui-demo",
    "javascript/apps/ui-demo-tutorial",
    "javascript/server"
  ],
  "engines": {
    "node": ">= 8",
    "yarn": ">= 1.0"
  },
  "scripts": {
    "config:check": "./bin/config-check",
    "make:tutorial": "./bin/make-tutorial",
    "start:server": "./bin/config-check && cd javascript/server && node server.js",
    "start:code": "./bin/config-check && cd javascript/apps/code-demo && react-scripts start",
    "start:node": "./bin/config-check && cd javascript/apps/node-demo && node main.js",
    "start:ui": "./bin/config-check && cd javascript/apps/ui-demo && react-scripts start",
    "start:ui-tutorial": "./bin/config-check && cd javascript/apps/ui-demo-tutorial && react-scripts start",
    "start:code-demo": "yarn start:code",
    "start:node-demo": "yarn start:node",
    "start:ui-demo": "yarn start:ui",
    "start:ui-demo-tutorial": "yarn start:ui-tutorial"
  },
  "devDependencies": {
    "fs-extra": "^5.0.0"
  }
}
